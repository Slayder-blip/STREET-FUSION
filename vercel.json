{
  "rewrites": [
    {
      "source": "/",
      "destination": "/Accueil/index.html"
    },
    {
      "source": "/localisation",
      "destination": "/Localisation/pageLocalisation/localisation.html"
    },
    {
      "source": "/menus",
      "destination": "/Menus/menus.html"
    },
    {
      "source": "/services",
      "destination": "/Privatisation/services.html"
    },
    {
      "source": "/(.*)",
      "destination": "/Accueil/index.html"
    }
  ]
}